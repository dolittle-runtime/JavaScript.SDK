{
    "files.exclude": {
        
        "**/jspm_packages": true,
        "**/node_modules": true,
        "**/.git": true,
        "**/.svn": true,
        "**/.DS_Store": true
    },
    "cSpell.words": [
        "grpc",
        "protobuf"
    ],

    "editor.formatOnSave": false,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "eslint.alwaysShowStatus": true
}